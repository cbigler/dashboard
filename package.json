{
  "name": "web-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "node-sass": "^4.5.3",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "start": "concurrently \"npm run start:react\" \"npm run start:scss\" ",
    "start:react": "react-scripts start",
    "start:scss": "node-sass --recursive --output src/built-css/ src/styles.scss && node-sass --watch --recursive --output src/built-css/ --source-map true --source-map-contents src/styles.scss",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",


    "make-component": "./utilities/make-component"
  }
}
